import vm/translator;
import vm/vm;

main() {
    file = "tests/primes/sieve";
    s = getFileContent(file + ".nemo");
    nemoProg = str2nemoProg(s);
    switch (nemoProg: Maybe<NemoProg>) {
        Some(pr): {
            vnemoProg = nemo2vnemo(pr);
            setFileContent(file + ".vnemo", vnemoProg2str(vnemoProg));
            exec(vnemoProg);
            quit(0);
        };
        None(): {
            println("None");
            quit(1);
        }
    }
}